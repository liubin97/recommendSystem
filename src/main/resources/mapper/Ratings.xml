<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.recommend.dao.RatingsDao">
    <select id="selectRating" parameterType="long" resultType="com.example.recommend.entity.Ratings">
        select * from ratings where movieId = #{movieId}
    </select>


</mapper>